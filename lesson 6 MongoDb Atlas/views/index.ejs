<html>

<head>
  <title>
    <link rel="stylesheet" href="/Style.css">
    <%= title %>
  </title>
</head>

<body>
  <%- include('./partials/navbar') %>
    <h1>Welcome, <%= user.name %>!</h1>
    <ul>
      <% items?.forEach(item=> { %>
        <li>
          <h1>
            <%= item.title %>
          </h1>
          <p><%= item.body %></p>
        </li>
        <% }) %>
    </ul>
</body>

</html>